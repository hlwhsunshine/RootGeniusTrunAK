<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="7dp"/>
            <gradient android:startColor="#ffbababa" android:endColor="#ffbababa" android:angle="90" android:centerY="0.75"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="7dp"/>
                <gradient android:startColor="#ffe9e9e9" android:endColor="#ffe9e9e9" android:angle="90" android:centerY="0.75"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="#ff1fbe84" android:endColor="#ff1fbe84" android:angle="270" android:centerY="0.75"/>
            </shape>
        </clip>
    </item>
</layer-list>
