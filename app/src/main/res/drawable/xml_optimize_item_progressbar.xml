<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="#fff0f0f0"/>
            <corners android:radius="10000dp"/>
            <stroke android:width="1dp" android:color="#ff1fbf69"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <solid android:color="#ff1fbf69"/>
                <corners android:radius="10000dp"/>
                <stroke android:width="1dp" android:color="#ff1fbf69"/>
            </shape>
        </clip>
    </item>
</layer-list>
